{
    "compilerOptions": {
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "isolatedModules": true,
      "jsx": "react",
      "lib": ["es6"],
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "noEmit": true,
      "strict": true,
      "target": "esnext",
      "baseUrl": ".",
      "paths": {
        "@/*": ["./src/*"],
        "@views/*": ["./src/views/*"],
        "@components/*": ["./src/components/*"],
      },
    },
    "exclude": [
      "node_modules",
      "babel.config.js",
      "metro.config.js",
      "jest.config.js"
    ],
    "include":[
      "src/**/*"
    ],
}